<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.example.crm.mapper.travel.TravelInsuranceMapper">
    <select id="selectAll" resultType="org.example.crm.entity.travel.TravelInsurance">
        SELECT * FROM travel_insurance;
    </select>

    <select id="selectById" resultType="org.example.crm.entity.travel.TravelInsurance">
        SELECT * FROM travel_insurance WHERE id = #{id};
    </select>
</mapper>